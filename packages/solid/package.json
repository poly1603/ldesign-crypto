{
  "name": "@ldesign/crypto-solid",
  "version": "2.0.0",
  "description": "Solid.js adapter for @ldesign/crypto-core",
  "keywords": ["solid", "solidjs", "crypto", "encryption"],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./es/index.d.ts",
      "import": "./es/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "main": "./lib/index.cjs",
  "module": "./es/index.js",
  "types": "./es/index.d.ts",
  "files": ["es", "lib", "README.md"],
  "scripts": {
    "build": "ldesign-builder build",
    "dev": "ldesign-builder build --watch"
  },
  "dependencies": {
    "@ldesign/crypto-core": "workspace:*"
  },
  "peerDependencies": {
    "solid-js": "^1.8.0"
  },
  "devDependencies": {
    "@ldesign/builder": "workspace:*",
    "solid-js": "^1.8.0",
    "typescript": "^5.7.3"
  }
}

